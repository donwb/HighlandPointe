back-color = white
back-body-color = white
back-h3-color = white
div-border-color = #2a6da9

border-radius(n)
  -webkit-border-radius n
  -moz-border-radius n
  border-radius n
  
gradient(a, b)
  background -moz-linear-gradient(top, a, b)
  background -webkit-gradient(linear, left top, left bottom, from(a), to(b))

header-border()
  border-style solid
  border-color div-border-color
  border-width 1px

font-adjust(n)
  font-size n
  font-family:Arial, Helvetica, Utkal, sans-serif

h3-adjust()
  color white
  font-family:Arial, Helvetica, Utkal, sans-serif

soc-image-adjust()
  height 50px
  width 50px

body
  font-adjust(12px)
  background-color back-body-color

text
  font-adjust(14px)

a
  font-adjust(14px)
  padding-left 5px

p
  font-adjust(14px)

